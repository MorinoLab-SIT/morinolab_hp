{
  "name": "morinolab-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint_all": "npm run lint -w morinolab_hp && npm run lint -w morinolab_cms",
    "generate_contents": "mkdir -p morinolab_hp/public/generated_contents/ && cp -r contents/* morinolab_hp/public/generated_contents/",
    "format_all": "npm run format -w morinolab_hp && npm run format -w morinolab_cms",
    "build_morinolab_hp": "npm run build -w morinolab_hp",
    "build_morinolab_cms": "npm run build -w morinolab_cms",
    "package_cms_linux_x64": "npm run pack-linux -w morinolab_cms",
    "package_cms_win32_x64": "npm run pack-win -w morinolab_cms",
    "package_cms_darwin_x64": "npm run pack-mac-x64 -w morinolab_cms",
    "package_cms_darwin_arm64": "npm run pack-mac-arm64 -w morinolab_cms"
  },
  "workspaces": [
    "morinolab_hp",
    "morinolab_cms"
  ]
} 